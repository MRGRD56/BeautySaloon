<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:BeautySaloon.Desktop.Styles">

    <Color x:Key="SecondaryBackgroundColor" R="255" G="228" B="255" A="255"/>
    <Color x:Key="SecondaryBackgroundLightColor" R="255" G="228" B="255" A="200"/>
    <Color x:Key="SecondaryBackgroundDarkColor" R="245" G="210" B="245" A="255"/>
    <Color x:Key="PrimaryColor" R="255" G="74" B="109" A="255"/>

    <SolidColorBrush x:Key="SecondaryBackgroundBrush" Color="{StaticResource SecondaryBackgroundColor}"/>
    <SolidColorBrush x:Key="SecondaryBackgroundLightBrush" Color="{StaticResource SecondaryBackgroundLightColor}"/>
    <SolidColorBrush x:Key="SecondaryBackgroundDarkBrush" Color="{StaticResource SecondaryBackgroundDarkColor}"/>
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}"/>
</ResourceDictionary>